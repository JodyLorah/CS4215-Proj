{
  "compilerOptions": {
    "outDir": "./evaluator/build",
    "module": "commonjs",
    "target": "es6",
    "sourceMap": true,
    "lib": [
      "dom",
      "es6",
      "scripthost",
      "es2015.collection"
    ],
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowJs": true,
    "strict": false,
    "noImplicitAny": false,
    "resolveJsonModule": true
  },
  "include": ["evaluator"],
  "exclude": [
    "node_modules"
  ]
}